  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/exec": "^1.0.4"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@zeit/ncc": "^0.22.3",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5",
    "typescript-formatter": "^7.2.2"
  }
